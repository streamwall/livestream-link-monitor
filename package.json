    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@jest/globals": "^29.7.0",
    "eslint": "^9.30.1",
    "eslint-plugin-jest": "^28.14.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "sinon": "^21.0.0",
    "supertest": "^7.1.1"
  }
