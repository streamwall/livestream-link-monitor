# Discord Configuration
DISCORD_TOKEN=your_discord_bot_token_here
DISCORD_CHANNEL_ID=your_discord_channel_id_here

# Twitch Configuration
TWITCH_CHANNEL=channel_name_without_hash

# Google Sheets Configuration (DEPRECATED - Use StreamSource instead)
# GOOGLE_SHEET_ID=your_google_sheet_id_here
# GOOGLE_CREDENTIALS_PATH=./credentials.json

# Optional Sync Intervals (milliseconds)
IGNORE_LIST_SYNC_INTERVAL=10000
EXISTING_URLS_SYNC_INTERVAL=60000
KNOWN_CITIES_SYNC_INTERVAL=300000

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=10

# Google Sheets Configuration (DEPRECATED - These are no longer used)
# SHEET_TAB_LIVESTREAMS=Livesheet
# SHEET_TAB_TWITCH_IGNORE=Twitch User Ignorelist
# SHEET_TAB_DISCORD_IGNORE=Discord User Ignorelist
# SHEET_TAB_URL_IGNORE=URL Ignorelist
# SHEET_TAB_KNOWN_CITIES=Known Cities
# COLUMN_SOURCE=Source
# COLUMN_PLATFORM=Platform
# COLUMN_STATUS=Status
# COLUMN_LINK=Link
# COLUMN_ADDED_DATE=Added Date
# COLUMN_POSTED_BY=Posted By
# COLUMN_CITY=City
# COLUMN_STATE=State

# Status Values
STATUS_NEW_LINK=Live

# Other Configuration
TIMEZONE=America/Los_Angeles
LOG_LEVEL=info
LOG_FILE=app.log

# Confirmation Settings (set to false to disable)
DISCORD_CONFIRM_REACTION=true
TWITCH_CONFIRM_REPLY=true

# Backend Configuration
# Mode: 'single' (use primary only), 'dual-write' (write to all enabled), 'migrate' (transition mode)
BACKEND_MODE=single
# Primary backend: 'googleSheets' or 'streamSource'
BACKEND_PRIMARY=streamSource
# Enable/disable specific backends
BACKEND_GOOGLE_SHEETS_ENABLED=false
BACKEND_STREAMSOURCE_ENABLED=true

# StreamSource Configuration (REQUIRED)
STREAMSOURCE_API_URL=https://api.streamsource.com
STREAMSOURCE_EMAIL=your_streamsource_email
STREAMSOURCE_PASSWORD=your_streamsource_password